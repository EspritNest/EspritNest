{% extends 'front-base.html.twig' %}

{% block title %}Logement{% endblock %}

{% block body %}
    <h1>Logement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ logement.id }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ logement.Adresse }}</td>
            </tr>
            <tr>
                <th>Code_postal</th>
                <td>{{ logement.CodePostal }}</td>
            </tr>
            <tr>
                <th>Superficie</th>
                <td>{{ logement.superficie }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ logement.Description }}</td>
            </tr>
            <tr>
                <th>Date_ajout</th>
                <td>{{ logement.dateAjout ? logement.dateAjout|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>ProprietaireId</th>
                <td>{{ logement.ProprietaireId }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_logement_index') }}">back to list</a>

    <a href="{{ path('app_logement_edit', {'id': logement.id}) }}">edit</a>

    {{ include('logement/_delete_form.html.twig') }}
{% endblock %}
